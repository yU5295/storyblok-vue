{
  "name": "universityofthemessage",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel develop watch:styl",
    "develop": "env-cmd -f .env nuxt-ts",
    "build": "npm-run-all --parallel build:css deploy",
    "start": "nuxt-ts start",
    "test": "jest",
    "deploy": "nuxt-ts build",
    "generate": "nuxt-ts generate",
    "watch:styl": "stylus -w assets/styl/styles.styl -o assets/css/styles.css",
    "build:css": "npm-run-all compile:styl prefix:css compress:css",
    "compile:styl": "stylus assets/styl/styles.styl -o assets/css/styles.prod.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' assets/css/styles.prod.css -o assets/css/styles.prod.css",
    "compress:css": "stylus -c assets/css/styles.prod.css"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "5.15.1",
    "@marvr/storyblok-rich-text-vue-renderer": "2.0.1",
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/axios": "5.13.1",
    "@nuxtjs/style-resources": "1.0.0",
    "@tailwindcss/typography": "0.3.1",
    "@vue-hero-icons/outline": "1.7.2",
    "@vue/composition-api": "1.0.0-beta.22",
    "core-js": "^3.6.5",
    "nuxt": "^2.14.6",
    "nuxt-fontawesome": "0.4.0",
    "nuxt-i18n": "6.26.0",
    "share-buttons": "1.7.0",
    "storyblok-markdown-richtext": "1.0.1",
    "storyblok-nuxt": "1.3.0",
    "vue-pagination-2": "3.0.8",
    "yt-player": "3.5.0"
  },
  "devDependencies": {
    "@fortawesome/free-brands-svg-icons": "5.15.1",
    "@nuxt/types": "^2.14.6",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/tailwindcss": "^3.1.0",
    "@types/jest": "26.0.23",
    "@types/ramda": "0.27.40",
    "@vue/test-utils": "1.2.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "env-cmd": "10.1.0",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-vue": "7.9.0",
    "jest": "26.6.3",
    "jest-environment-jsdom-sixteen": "2.0.0",
    "jest-environment-node": "26.6.2",
    "jest-serializer-vue": "2.0.2",
    "npm-run-all": "4.1.5",
    "postcss": "8.2.12",
    "postcss-cli": "8.3.1",
    "prettier": "2.2.1",
    "pug": "3.0.0",
    "pug-loader": "2.4.0",
    "pug-plain-loader": "1.1.0",
    "ramda": "0.27.1",
    "stylus": "0.54.8",
    "stylus-loader": "3.0.2",
    "ts-jest": "26.5.6",
    "vue-jest": "3.0.7",
    "vuex-composition-helpers": "1.0.23"
  }
}
